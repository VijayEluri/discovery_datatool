<config>
  <datasource>
    <mysqlTest>
      <url>jdbc:mysql://localhost/test</url>
      <user>test</user>
      <password>test</password>
    </mysqlTest>
  </datasource>

  <publisher>
    <test>
      <extractor>com.t11e.discovery.datatool.SqlChangesetExtractor</extractor>
      <dataSource>mysqlTest</dataSource>
      <actions>
        <action>
          <action>create</action>
          <filter>delta</filter>
          <query><![CDATA[
            select * from test_table
            where last_updated >= :start and last_updated < :end
          ]]></query>
          <idColumn>id</idColumn>
          <jsonColumnNames>jsonColumn</jsonColumnNames>
        </action>
        <action>
          <action>create</action>
          <filter>snapshot</filter>
          <query>select * from Content</query>
          <idColumn>id</idColumn>
          <jsonColumNames>data</jsonColumNames>
        </action>
      </actions>
    </test>
  </publisher>
</config>
